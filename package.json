{
  "name": "wprei",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "gulpwatch": "gulp watch",
    "gulpstyles": "gulp styles",
    "gulpscripts": "gulp scripts",
    "watch:sass": "node-sass app/wp-content/themes/resonance_theme/sass/main.scss app/wp-content/themes/resonance_theme/css/style.css -w",
    "devserver": "live-server --open=resonance.test",
    "start": "npm-run-all --parallel gulpwatch  watch:sass",
    "compile:sass": "node-sass sass/main.scss css/style.comp.css",
    "concat:css": "concat -o css/style.concat.css css/icon-font.css css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' css/style.concat.css -o css/style.prefix.css",
    "compress:css": "node-sass css/style.prefix.css css/style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass concat:css prefix:css compress:css"
  },
  "author": "Inkant Awasthi",
  "repository": {
    "type": "git",
    "url": "https://github.com/tim1016/wprei.git"
  },
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "7.0.0-beta.34",
    "@babel/preset-env": "7.0.0-beta.34",
    "autoprefixer": "^8.1.0",
    "babel-loader": "8.0.0-beta.0",
    "browser-sync": "^2.23.6",
    "contact": "^0.2.5",
    "del": "^3.0.0",
    "gulp": "^3.9.1",
    "gulp-postcss": "^7.0.1",
    "gulp-rename": "^1.2.2",
    "gulp-svg-sprite": "^1.4.0",
    "gulp-svg2png": "^2.0.2",
    "gulp-watch": "^5.0.0",
    "node-sass": "^4.7.2",
    "npm-run-all": "^4.1.2",
    "postcss-cli": "^5.0.0",
    "svg2png": "^3.0.1",
    "webpack": "3.10.0"
  },
  "dependencies": {
    "jquery": "3.2.1",
    "normalize.css": "7.0.0",
    "slick-carousel": "1.8.1"
  }
}
